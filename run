<script>
function decode(str)
{
    var txt = new DOMParser().parseFromString(str, "text/html");
    return txt.documentElement.textContent;
}

$(document).ready(function(){
    $('#accordion a.item').click(function(){
        $('#accordion li').children('ul').slideUp('fast');
        $("#accordion a").removeClass("active");
        $(this).addClass("active");
        $(this).siblings('ul').slideDown('fast');
        return false;
    });

    $('#HotSpot').hide();


    $("table#status tr:odd").hide();
    $("table#status tr:even td.status-link a").bind('click', function(event){
        event.preventDefault();

        var $this = $(this);
        var $tr = $(this).closest("tr").next();

        if($tr.css("display") == "none")
        {
            $tr.show();
            $this.html("Esconder");
        }
        else
        {
            $tr.hide();
            $this.html("Detalhes");
        }

        return false;
    });

    $(".tab").slideUp();
        $("#tabip-01").slideDown();
        $("#menu-ip-mode li:first a").addClass("active");

        $("#menu-ip-mode  li a").click(function(){
            var i = $(this).attr("rel");
            $("#menu-ip-mode  li a").removeClass("active");
            $(this).addClass("active");
            $(".tab").slideUp();
            $("#tabip-"+i).slideDown();
            return false;
        });
});
</script>

